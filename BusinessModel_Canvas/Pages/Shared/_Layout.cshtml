<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Business Model Canvas</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/bootstrap-4.3.1-dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/bootstrap-4.3.1-dist/css/bootstrap-grid.css" />
    <link rel="stylesheet" href="~/bootstrap-4.3.1-dist/css/bootstrap-reboot.css" />
    <link rel="stylesheet" href="~/css/site.css" />
   

</head>
<body style="padding-top: 0px;">


    <nav id="main-navbar" class="navbar navbar-expand-lg navbar-dark navbar-static-top row">

    </nav>

    <div id="subtitle" class="col position-static">
        <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>
        Loading...
    </div>


    <div id="SelectMenu" class="navbar-fixed-top ">

        <nav id="select-navbar" class="navbar navbar-expand-lg navbar-dark navbar-fixed-top ">



            <div id="MainMenu" class="container row navbar-fixed-top ">
                <div id="MenuLogo" class="col container">
                    <img src='\images\g4568.png' />
                </div>
                <div class="navbar-nav mr-auto col container">
                    <a id="main-title" class="navbar-brand" asp-page="/Index">Business Model Canvas</a>
                </div>

                <div id="SubMenu" class="nav-item container col">



                    <button id="ToggleButton" class="navbar-brand" type="button" data-toggle="collapse" data-target="#MainArea" aria-controls="#MainArea" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>


                </div>

            </div>


        </nav>

        <div class="pos-f-t" style="width:100%;">


            <div class="collapse" id="MainArea">
                <div id="main-menu-top" class="navbar navbar-expand-lg navbar-dark navbar-fixed-top "></div>
                <div class="" id="CanvasSelectContainer" style="width:100%;">

                    <div class="" id="CanvasSelect">
                        <div class="row" id="TopSelect" style="width:100%;margin-left: 0px;margin-right: 0px;">

                            <div class="col-lg-2" style="padding-left:0px;padding-right:0px;">
                                <button class="btn btn-turkcell-outline border2 border22" style="width: 100%;height:200px;" onclick=" openNav(innerHTML);">KEY PARTNERS</button>
                            </div>

                            <div class="col-lg-2">
                                <div class="row"> <button class="btn btn-turkcell-outline border2 border22" style="width: 100%;height:100px;" onclick=" openNav(innerHTML);">KEY ACTIVITIES</button></div>
                                <div class="row"> <button class="btn btn-turkcell-outline border2 border22" style="width: 100%;height:100px;" onclick=" openNav(innerHTML);">KEY RESOURCES</button></div>
                            </div>

                            <div class="col-lg-2" style="padding-left:0px;padding-right:0px;">
                                <button class="btn btn-turkcell-outline border2 border22" style="width: 100%;height:200px" onclick=" openNav(innerHTML);">VALUE PROPOSITION</button>
                            </div>

                            <div class="col-lg-2">
                                <div class="row"> <button class="btn btn-turkcell-outline border2 border22" style="width: 100%;height:100px" onclick=" openNav(innerHTML);">COSTUMER RELATIONSHIPS</button></div>
                                <div class="row"> <button class="btn btn-turkcell-outline border2 border22" style="width: 100%;height:100px" onclick=" openNav(innerHTML);">CHANNELS</button></div>
                            </div>

                            <div class="col-lg-2" style="padding-left:0px;padding-right:0px;">
                                <button class="btn btn-turkcell-outline border2 border22" style="width: 100%;height:200px" onclick=" openNav(innerHTML);">CUSTOMER SEGMENT</button>
                            </div>




                        </div>


                        <div class="row" id="BottomSelect" style="width:100%;margin-left: 0px;margin-right: 0px;">
                            <div class="col-lg-5" style="padding-left: 0px;padding-right: 0px;">

                                <button class="btn btn-turkcell-outline border2 border22" style="width: 100%;height:100px;padding-left:0px;padding-right:0px;" onclick=" openNav(innerHTML);">COST STRUCTURE</button>

                            </div>
                            <div class="col-lg-5" style="padding-left: 0px;padding-right: 0px;">

                                <button class="btn btn-turkcell-outline border2 border22" style="width: 100%;height:100px;padding-left:0px;padding-right:0px;" onclick=" openNav(innerHTML);">REVENUE STREAMS</button>

                            </div>
                        </div>
                    </div>
                </div>
                <nav id="main-menu-bottom" class="navbar navbar-expand-lg navbar-dark navbar-fixed-top "></nav>
            </div>





        </div>



    </div>







    <!--Content below-->

    <div class="body-content">
        @RenderBody()
        <hr />


        <footer>
            <p style="/*font-size:50px*/">&copy; 2019 - CanvasBussinessModel by Oğuz Kaan Özsoy <!--and Erdal Çetin--></p>
        </footer>

    </div>

    <script src="~/js/vendor/jquery.js"></script>
    <script src="~/js/carhartl-jquery-cookie-92b7715/jquery.cookie.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.15.0/umd/popper.js" integrity="sha256-7BfFV/dSvQT4pGBvRAIt6JDXsehb92DQqpGUndLCPQ4=" crossorigin="anonymous"></script>
    <script src="~/bootstrap-4.3.1-dist/js/bootstrap.js"></script>
    <script src="~/js//jquery-ui-1.12.1.custom/jquery-ui-1.12.1.custom/jquery-ui.js"></script>



    <script>

        $(function () {
            var path = window.location.pathname;
            var title = "Business Model Canvas";
            switch (path) {
                case "/KEYPARTNERS": title = "/KEY PARTNERS"; break;
                case "/KEYACTIVITIES": title = "/KEY ACTIVITIES"; break;
                case "/KEYRESOURCES": title = "/KEY RESOURCES"; break; break;
                case "/VALUEPROPOSITION": title = "/VALUE PROPOSITION"; break;
                case "/COSTUMERRELATIONSHIPS": title = "/COSTUMER RELATIONSHIPS"; break;
                case "/CHANNELS": title = "/CHANNELS"; break;
                case "/CUSTOMERSEGMENT": title = "/CUSTOMER SEGMENT"; break;
                case "/COSTSTRUCTURE": title = "/COST STRUCTURE"; break;
                case "/REVENUESTREAMS": title = "/REVENUE STREAMS";
            }
            document.getElementById("subtitle").innerHTML = title;
        }
        );


        function openNav(innerHTML) {
            window.location.href = innerHTML.replace(/\s/g, '');
        }

        $(document).on('click', function (e) {

            var Menu = $('#MainArea').hasClass('show');

            if (!$(e.target).closest('#MainArea').length &&
                !$(e.target).is('#MainArea') &&
                Menu == true) {
                $('#MainArea').collapse('toggle');
            }

        });

        $('#myModal').on('shown.bs.modal', function () {
            $('element').trigger('focus')
        })
        function Search(element, target) {
            var data = $(element).val().toLowerCase();

            $(target).each(function (i, e) {

                if (!$(e).data('name').toLowerCase().includes(data) && !$(e).hasClass('active')) {
                    $(e).hide();
                } else {
                    $(e).show();
                }

            });

        }

    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
